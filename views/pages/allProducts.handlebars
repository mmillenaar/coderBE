<h1>All products</h1>
{{#each allProducts}}
<br/>
<ul>
    <h2>Product {{_id}}</h2>
    <img src={{thumbnail}} alt={{title}}>
    <li>Title: {{title}}</li>
    <li>Description: {{description}}</li>
    <li>Code: {{code}}</li>
    <li>Price: {{price}}</li>
    <li>Stock: {{stock}}</li>
</ul>
{{/each}}